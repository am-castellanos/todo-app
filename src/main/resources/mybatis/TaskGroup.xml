<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.imint.example.repository.TaskGroupRepository">

    <select id="findAll" resultType="com.imint.example.domain.TaskGroup">
        SELECT * FROM list
    </select>

    <select id="findById" parameterType="java.lang.Long" resultType="com.imint.example.domain.TaskGroup">
        SELECT * FROM list WHERE id = #{id}
    </select>

</mapper>
